{
  "name": "Education-view",
  "version": "6.1.0",
  "author": "everis",
  "license": "MIT",
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "assert": false,
    "buffer": false,
    "util": false,
    "child_process": false,
    "console": false,
    "events": false
  },
  "scripts": {
    "start": "(IF EXIST wwwroot (rmdir /S /Q wwwroot)) && npm run build:watch",
    "start:web": "ng serve -o",
    "start:prod": "npm run build:prod && npm run electron:prod",
    "electronDep": "cd wwwroot && npm install --configuration=production && cd..",
    "electron": "electron-builder install-app-deps && npm run electronDep && npm run electron:start",
    "electron:start": "electron ./wwwroot/electron --dev",
    "electron:prod": "npm run electronDep && electron ./wwwroot/electron",
    "build:watch": "ng build --watch --source-map --delete-output-path=false",
    "build:prod": "ng build --configuration=production && npm run electronDep",
    "build:ci": "ng build --configuration=production",
    "build:web:prod": "ng build --configuration=production --base-href ./",
    "package:win": "npm run build:prod && cross-var electron-packager wwwroot $npm_package_name --out=packages --arch=x64 --overwrite --platform=win32 --icon=src/assets/icons/Education_view_icon.ico",
    "package:linux": "npm run build:prod && cross-var electron-packager wwwroot $npm_package_name --out=packages --arch=x64 --overwrite --platform=linux",
    "package:osx": "npm run build:prod && cross-var electron-packager wwwroot $npm_package_name --out=packages --arch=x64 --overwrite --platform=darwin",
    "package:all": "npm run build:prod && cross-var electron-packager wwwroot $npm_package_name --out=packages --arch=x64 --overwrite --all",
    "test": "ng test",
    "test:exit": "ng test --watch=false",
    "test:vsts": "ng test --code-coverage --watch=false --karma-config ./src/karma.conf-vsts.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "translate:extract": "ngx-translate-extract --input ./src --output ./tmp/translations-keys.json --sort --key-as-default-value --replace --format json",
    "docs": "compodoc -w -p src/tsconfig.app.json",
    "storybook": "ng run trade-Education-platform-tdp:storybook",
    "build-storybook": "ng run trade-Education-platform-tdp:build-storybook"
  },
  "dependencies": {
    "@angular/animations": "17.0.5",
    "@angular/cdk": "16.2.12",
    "@angular/common": "17.0.5",
    "@angular/compiler": "17.0.5",
    "@angular/core": "17.0.5",
    "@angular/forms": "17.0.5",
    "@angular/material": "16.2.12",
    "@angular/platform-browser": "17.0.5",
    "@angular/platform-browser-dynamic": "17.0.5",
    "@angular/router": "17.0.5",
    "@azure/msal-angular": "^3.0.4",
    "@azure/msal-browser": "^3.1.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "3dc-middleware": "file:../SpecialEducationPlanning
.EducationMiddleware/3DC.Middleware",
    "angular2-notifications": "16.0.1",
    "fusion-middleware": "file:../SpecialEducationPlanning
.EducationMiddleware/Fusion.Middleware",
    "ng-block-ui": "^4.0.1",
    "ng2-pdf-viewer": "^10.4.0",
    "offline-middleware": "file:../SpecialEducationPlanning
.EducationMiddleware/Offline.Middleware",
    "portscanner": "2.2.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.5",
    "@angular-eslint/builder": "^17.1.1",
    "@angular-eslint/eslint-plugin": "^17.1.1",
    "@angular-eslint/eslint-plugin-template": "^17.1.1",
    "@angular-eslint/schematics": "^17.1.1",
    "@angular-eslint/template-parser": "^17.1.1",
    "@angular/cli": "17.0.5",
    "@angular/compiler-cli": "17.0.5",
    "@angular/language-service": "17.0.5",
    "@babel/core": "^7.23.0",
    "@compodoc/compodoc": "^1.1.23",
    "@electron/rebuild": "^3.3.0",
    "@storybook/addon-actions": "^7.6.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/addon-storyshots": "^7.6.0",
    "@storybook/angular": "^7.6.0",
    "@types/jasmine": "^5.1.4",
    "@types/jasminewd2": "^2.0.13",
    "@types/node": "^18.18.12",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vendure/ngx-translate-extract": "^9.0.3",
    "cross-var": "file:deps/cross-var-1.1.1.tgz",
    "electron": "20.3.8",
    "electron-builder": "^24.6.4",
    "electron-packager": "^17.1.2",
    "electron-reload": "1.5.0",
    "eslint": "^8.39.0",
    "jasmine-core": "^5.1.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "protractor": "7.0.0",
    "sass": "1.29.0",
    "ts-node": "5.0.1",
    "typescript": "5.2.2"
  },
  "transform": {
    "^.+\\.(ts|html)$": "ts-jest",
    "^.+\\.js$": "babel-jest",
    "^.+\\.stories\\.[jt]sx?$": "@storybook/addon-storyshots/injectFileName"
  }
}

